@startuml

actor HR as "HR Advisor"

rectangle Database

rectangle "HR System" {
    usecase UC4 as "Get salaries
    by role"
     usecase UC5 as "Get salaries
        by department"

    usecase UCa as "Print salaries"

    HR - UC5
    UC4 ..> UCa : include
    UC5 ..> UC4
    UC5 - Database
}

@enduml
